  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js"
    }
  },
  "prisma": {
    "seed": "ts-node ./src/seed.ts"
  },
  "devDependencies": {
    "prisma": "^6.3.0"
  },
  "dependencies": {
    "@prisma/client": "6.3.0"
  }
}
