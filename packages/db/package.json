{
  "name": "@skydock/db",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "db:generate": "prisma generate && tsc",
    "db:migrate": "prisma migrate dev --skip-generate",
